{
  "name": "@karelkangro/deno-ai-toolkit-ui",
  "version": "1.0.1",
  "description": "Admin UI components for Deno AI Toolkit workspace management",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "precommit": "yarn type-check && yarn lint && yarn format:check",
    "prepare": "husky",
    "prepublishOnly": "yarn precommit && yarn build"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.90.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tanstack/react-query": "^5.90.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "daisyui": "^5.3.2",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0"
  },
  "keywords": [
    "deno",
    "ai",
    "workspace",
    "admin",
    "react",
    "ui",
    "components",
    "lancedb",
    "vector-database",
    "rag",
    "ai-toolkit"
  ],
  "author": "Karel Kangro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karelkangro/deno-ai-toolkit-ui.git"
  },
  "bugs": {
    "url": "https://github.com/karelkangro/deno-ai-toolkit-ui/issues"
  },
  "homepage": "https://github.com/karelkangro/deno-ai-toolkit-ui#readme",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
